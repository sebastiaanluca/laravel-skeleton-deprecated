{
    "name": "sebastiaanluca/laravel-skeleton",
    "description": "Laravel skeleton application",
    "keywords": [
        "framework",
        "laravel",
        "skeleton",
        "scaffolding"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.8",
        "barryvdh/laravel-debugbar": "^2.2.3",
        "graham-campbell/exceptions": "^9.0",
        "laravel/framework": "~5.3.19",
        "laravelcollective/html": "^5.3",
        "nesbot/carbon": "^1.21",
        "nwidart/laravel-modules": "dev-feature/refactor",
        "predis/predis": "^1.0",
        "radic/blade-extensions": "^6.2",
        "rinvex/repository": "dev-feature/add-entities as 2.0.1",
        "sebastiaanluca/laravel-conditional-providers": "dev-develop",
        "sebastiaanluca/laravel-helpers": "dev-develop",
        "sebastiaanluca/laravel-router": "dev-develop",
        "sebastiaanluca/laravel-unbreakable-migrations": "dev-develop",
        "sebastiaanluca/laravel-validator": "dev-develop"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^5.4",
        "raveren/kint": "^1.0",
        "symfony/css-selector": "^3.0",
        "symfony/dom-crawler": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:sebastiaanluca/laravel-modules.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sebastiaanluca/repository.git"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Theme\\": "modules/theme/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "composer dumpautoload -o",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "composer dumpautoload",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "configure": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
